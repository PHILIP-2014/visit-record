##set($layout="/layout/blank.vm")

<div id="preview_error">
#if(${error})
	<div  class="inaccessible-page">
        <img src="#springUrl('/images/email_verify_0.png')">
        <p>${error}</p><!-- style="margin-top:120px" -->
    </div>
#else
	<div  class="inaccessible-page">
		<img src="#springUrl('/images/email_verify_1.png')">
        <p>您的账号已验证，<a href="#springUrl('/login')">点击这里</a> 登录系统。</p><!-- style="margin-top:120px" -->
    </div>
#end
</div>
