<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.church.dao.UserDao" >
	
	<sql id="default_column">
		`user`.`id`,
		`user`.`name`,
		`user`.`phone`,
		`user`.`password`,
		`user`.`age`,
		`user`.`address`,
		`user`.`type`,
		`user`.`is_disable` as isDisable,
		`user`.`gmt_created` as gmtCreated,
		`user`.`gmt_modify` as gmtModify
	</sql>
	
	<insert id="insert" parameterType="com.church.model.User" keyProperty="id">
		insert into b_user (
			`name`,
			`phone`,
			`password`,
			`age`,
			`address`,
			`type`,
			`is_disable`,
			`gmt_created`,
			`gmt_modify`)
		values (
			#{name},
			#{phone},
			#{password},
			#{age},
			#{address},
			#{type},
			#{isDisable},
			now(),now())
	</insert>
	
</mapper>